@model List<Category>

<div class="list-group">
    <a asp-controller="Product" asp-action="List" asp-route-itemid="" class="list-group-item list-group-action">Tum
        Kategoriler</a>
    @foreach (var category in Model)
    {
        <a asp-controller="Product" asp-action="List" asp-route-itemid="@category.CategoryId"
            class="list-group-item list-group-action">@category.Name</a>
    }
</div>